{
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    ".env": ".env.*",
    ".gitconfig": ".gitattributes, .gitignore, .gitmessage",
    "*.module.ts": "$(capture).*.ts",
    "*.ts": "$(capture).spec.ts, $(capture).test.ts",
    "tsconfig.json": "tsconfig.*.json, env.d.ts, typedoc.*",
    "vite.config.*": "vite.*.config.ts, jsconfig*, vitest.config.*, cypress.config.*, playwright.config.*, *.d.ts",
    "docker-compose.*": "Dockerfile*, .dockerignore",
    "package.json": "package-lock.json, pnpm*, .yarnrc*, yarn*, .eslint*, eslint*, .prettier*, prettier*, .editorconfig, steiger.config.ts, steiger.config.*"
  },
  "cSpell.words": [
    "dynamoose",
    "codegen",
    "healthcheck",
    "konga",
    "pinia",
    "smui",
    "steiger"
  ]
}
